<Window x:Class="NSDMasterInventorySF.SpreadsheetEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NSDMasterInventorySF"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        mc:Ignorable="d"
        Title="Spreadsheet Editor" Height="700" Width="1200">
	<DockPanel>
		<syncfusion:SfSpreadsheetRibbon x:Name="Ribbon" DockPanel.Dock="Top"
		                                DataContext="{Binding ElementName=Spreadsheet}" />
		<syncfusion:SfSpreadsheet x:Name="Spreadsheet" />
	</DockPanel>
</Window>