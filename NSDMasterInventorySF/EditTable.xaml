<Window x:Class="NSDMasterInventorySF.EditTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        mc:Ignorable="d"
        Title="Edit Table" Height="200" Width="200" ResizeMode="NoResize" Loaded="OnLoaded">
    <StackPanel HorizontalAlignment="Center" Margin="10" FocusManager.FocusedElement="{Binding ElementName=TableNameBox}">
        <ComboBox IsDropDownOpen="True" Name="PrefabComboBox" Margin="0,0,0,30" />
        <syncfusion:SfTextBoxExt x:Name="TableNameBox" Watermark="Table Name" Margin="0,0,0,30" />
        <Button x:Name="ChooseButton" Content="Done" Click="DoneButtonOnClicked" />
    </StackPanel>
</Window>